<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="fls_question">  
    <typeAlias alias="question" type="com.falvshuo.bean.QuestionBean" />
	<resultMap class="question" id="rs_question">
		<result property="questionId" column="question_id" javaType="java.lang.Integer" jdbcType="NUMBER"/>
		<result property="questionNo" column="question_no" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="purpose" column="purpose" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="title" column="title" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="content" column="content" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="contentExtra" column="content_extra" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="source" column="source" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="fromUserId" column="from_user_id" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="phone" column="phone" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="realName" column="real_name" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="parentCaseType" column="parent_case_type" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="-1"/>
		<result property="childCaseType" column="child_case_type" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="-1"/>
		<result property="provinceId" column="province_id" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="cityId" column="city_id" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="createDate" column="create_date" javaType="java.lang.String" jdbcType="DATETIME"/>
		<result property="enabled" column="enabled" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="1"/>
		<result property="bestReplyId" column="best_reply_id" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="replyNum" column="reply_num" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="replyFirstDate" column="reply_first_date" javaType="java.lang.String" jdbcType="DATETIME"/>
		<result property="ifReplyInDay" column="if_reply_in_day" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="replyRecentDate" column="reply_recent_date" javaType="java.lang.String" jdbcType="DATETIME"/>
		<result property="ifSolved" column="if_solved" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="recommendReplyId" column="recommend_reply_id" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="fetchSource" column="fetch_source" javaType="java.lang.String" jdbcType="VARCHAR2"/>
		<result property="ifTags" column="if_tags" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
	</resultMap>
	<resultMap class="question" id="rs_question_more" extends="rs_question">
		<result property="childTypeName" column="childTypeName" javaType="java.lang.String" jdbcType="VARCHAR2"/>
	</resultMap>
	
	
	

	<insert id="toInsert" parameterClass="question">
		
	</insert>
	
	
	<select id="toList" resultMap="rs_question" parameterClass="string">
		select * from fls_question 
		$whereSql$
	</select>
	<select id="toCount" resultClass="java.lang.Integer" parameterClass="string">
		select count(*) from fls_question 
		$whereSql$
	</select>
	<select id="toCountBySql" resultClass="java.lang.Integer" parameterClass="string">
		$sql$
	</select>
	
	<update id="toUpdate" parameterClass="question">
		UPDATE fls_question set
		
		parent_case_type=#parentCaseType#,
		child_case_type=#childCaseType#,
		if_tags=#ifTags#
		enabled = #enabled#
		
		WHERE question_id=#questionId#
	</update>
	<update id="toUpdateBySql" parameterClass="string">
		$sql$
	</update>
	
	<delete id="toDelete" parameterClass="java.util.Map">
		
	</delete>
</sqlMap>