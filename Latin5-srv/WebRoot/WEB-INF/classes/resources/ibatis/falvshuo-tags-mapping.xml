		<result property="updateDate" column="update_date" javaType="java.lang.String" jdbcType="DATETIME"/>
		<result property="updateUserId" column="update_user_id" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="updateUserName" column="update_user_name" javaType="java.lang.String" jdbcType="VARCHAR2"/>
	</resultMap>
	
	
	
    <select id="toList" resultMap="rs_tags" parameterClass="string">
    	 select * from fls_tags
    	 $whereSql$
    </select>
    
    <select id="toCount" resultClass="java.lang.Integer" parameterClass="string" >
    	select count(*) from fls_tags 
    	$whereSql$
    </select>
    
    
    <update id="toUpdate" parameterClass="tags">
    	update fls_tags set
    	
    	match_num=#matchNum#
    	
		where tags_id = #tagsId#
    </update>
    
    <insert id="toInsert" parameterClass="tags">
    	
    </insert>
    
    <delete id="deleteTags" parameterClass="java.util.Map">
    </delete>
	
