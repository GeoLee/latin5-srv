		<result property="matchNum" column="match_num" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="0"/>
		<result property="orderNum" column="order_num" javaType="java.lang.Integer" jdbcType="NUMBER" nullValue="1"/>
